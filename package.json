{
	"name": "helloworld",
	"displayName": "HelloWorld",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onCommand:extension.testcmd"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.testcmd",
				"title": "Test CMD"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2",
		"@types/node": "^12.0.10",
		"gts": "^1.0.0",
		"typescript": "~3.4.0",
		"vscode": "^1.1.34",
		"impor": "^0.1.1",
		"compressing": "^1.4.0"
	},
	"dependencies": {
		"vscode-iot-device-cube-sdk": "^0.0.21"
	}
}
